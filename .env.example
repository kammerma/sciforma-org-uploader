
# Sciforma configuration
SCIFORMA_BASE_URL=https://your-sciforma-instance.example.com/api
SCIFORMA_TOKEN_URL=https://your-sciforma-instance.example.com/oauth/token
SCIFORMA_CLIENT_ID=your-client-id
SCIFORMA_CLIENT_SECRET=your-client-secret
SCIFORMA_SCOPE="organizations:read organizations:write"

# Service
LOG_LEVEL=INFO
REQUEST_TIMEOUT_SECONDS=30

# Rate limit (requests per second)
SCIFORMA_RATE_LIMIT_RPS=10